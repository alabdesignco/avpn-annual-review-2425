{
    "version": 2,
    "routes": [
      {
        "src": "/(.*\\.(js|css|json|txt|xml|svg|png|jpg|jpeg|gif|webp|ico))",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "outputDirectory": "dist"
  }